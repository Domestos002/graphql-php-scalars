{
  "name": "mll-lab/graphql-php-scalars",
  "description": "A collection of custom scalar types for usage with https://github.com/webonyx/graphql-php",
  "type": "library",
  "keywords": ["graphql", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": ">=7.1",
    "webonyx/graphql-php": "^0.13",
    "spatie/regex": "^1.3",
    "egulias/email-validator": "^2.1",
    "thecodingmachine/safe": "^0.1.14"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^7.3",
    "psy/psysh": "^0.9.8",
    "phpstan/phpstan": "^0.11.15"
  },
  "autoload": {
    "files": [
      "src/utils.php"
    ],
    "psr-4": {
      "MLL\\GraphQLScalars\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "stan": "phpstan analyse"
  }
}
